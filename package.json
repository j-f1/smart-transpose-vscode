{
  "name": "vscode-smart-transpose",
  "displayName": "Smart Transpose",
  "description": "Improves the built-in transpose command to work with more than individual characters",
  "version": "1.0.0",
  "publisher": "j-f1",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/j-f1/vscode-smart-transpose"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:transpose.smart"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "transpose.smart",
        "title": "Smart Transpose"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "prettier": "2.6.2",
    "typescript": "^4.6.3"
  }
}
